all: hello

hello: hello.o console.o directory.o dict.o avlBinTree.o sorting.o list.o circular_array.o memory.o fileIO.o array.o stack.o stacklist.o string.o queue.o avl_tree.o pointer.o index.o error.o argparse.o path.o alist.o config.o dynamic_array.o int.o
	clang -arch arm64 -Wall -o output/build/hello output/objects/hello.o output/objects/console.o output/objects/directory.o output/objects/dict.o output/objects/avlBinTree.o output/objects/sorting.o output/objects/list.o output/objects/circular_array.o output/objects/memory.o output/objects/fileIO.o output/objects/array.o output/objects/stack.o output/objects/stacklist.o output/objects/string.o output/objects/queue.o output/objects/avl_tree.o output/objects/pointer.o output/objects/index.o output/objects/error.o output/objects/argparse.o output/objects/path.o output/objects/alist.o output/objects/config.o output/objects/dynamic_array.o output/objects/int.o -g

hello.o: src/main/hello.c
	clang -arch arm64 -Wall -c -o output/objects/hello.o src/main/hello.c -g

