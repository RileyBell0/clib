all: main

main: main.o list.o tree.o error.o memory.o fileIO.o string.o index.o
	gcc -Wall -o output/build/main output/objects/main.o output/objects/list.o output/objects/tree.o output/objects/error.o output/objects/memory.o output/objects/fileIO.o output/objects/string.o output/objects/index.o -g

main.o: src/main/main.c
	gcc -Wall -c -o output/objects/main.o src/main/main.c -g

list.o: src/std/struc/c/list.c
	gcc -Wall -c -o output/objects/list.o src/std/struc/c/list.c -g

tree.o: src/std/struc/c/tree.c
	gcc -Wall -c -o output/objects/tree.o src/std/struc/c/tree.c -g

error.o: src/std/sys/c/error.c
	gcc -Wall -c -o output/objects/error.o src/std/sys/c/error.c -g

memory.o: src/std/c/memory.c
	gcc -Wall -c -o output/objects/memory.o src/std/c/memory.c -g

fileIO.o: src/std/c/fileIO.c
	gcc -Wall -c -o output/objects/fileIO.o src/std/c/fileIO.c -g

string.o: src/std/c/string.c
	gcc -Wall -c -o output/objects/string.o src/std/c/string.c -g

index.o: src/std/c/index.c
	gcc -Wall -c -o output/objects/index.o src/std/c/index.c -g

clean:
	rm output/objects/*.o output/build/*
